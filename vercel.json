{
  "version": 2,
  "name": "gatsby-theme-carbon-base",
  "alias": "gatsby-theme-carbon-base",
  "public": true,
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": {
        "distDir": "packages/example/public"
      }
    },
    {
      "src": "fonts/**/*.{woff2,ttf}",
      "use": "@now/static"
    }
  ]
}
